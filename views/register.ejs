<meta name="viewport" content="width=device-width, initial-scale=1.0">
<link rel="stylesheet" type="text/css"   href="/css/register.css">
<body>
<header class="header" id="head-container">
  <button class=hamburger id="Hamburger-button">
    <img src="images/icons8-menu-30 (1).png" alt=""/>
  </button>
  <div class="header-text">Rabbit Habit</div>
  <img
    class="bunnyImage"
    src="/images/bunny-inverse-removebg-preview(2).png"
    alt="Bunny image"
  />
</header>
<h1>Register</h1>
<form action="/register" method="POST" class="form">
    <div>
    <input type="UserName" id="name" name="name" placeholder="UserName" class="UserName">
    </div>
    <input type="text" name="email" placeholder="Email@test.com" class="email" id="email">
    <br>
    <input type="text" name="password" placeholder="Password" class="password" id="password">
    <div>
    <input type="submit" name="login" value="login" onclick="validate()" class="login-form-btn" id="login-form-btn">
    </div>
  </form>

  <footer>
    <footer class="footer">
      <img src="\images\gitcat.png" alt="" class="gitcatoj">
    </footer>
  </footer>


  <script src="js/index.js"></script>
</body>